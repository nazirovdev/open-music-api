{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-underscore-dangle": "off",
        "radix": "off",
        "no-console": "off",
        "no-shadow": "off",
        "no-unused-vars": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
